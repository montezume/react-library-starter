{
  "name": "react-library-starter",
  "version": "0.0.1",
  "description": "opinionated starter for creating react libraries",
  "main": "dist/react-library-starter.cjs.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/montezume/react-library-starter.git"
  },
  "module": "dist/react-library-starter.esm.js",
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "yarn build:es && yarn build:cjs && yarn build:umd",
    "build:umd": "cross-env NODE_ENV=production BUILD_FORMAT=esm rollup -c -f umd -n react-library-starter -o ./dist/react-library-starter.umd.js",
    "build:cjs": "cross-env NODE_ENV=production BUILD_FORMAT=cjs rollup -c -f cjs -o ./dist/react-library-starter.cjs.js",
    "build:es": "cross-env NODE_ENV=production BUILD_FORMAT=esm rollup -c -f esm -o ./dist/react-library-starter.esm.js",
    "lint": "jest --config jest.eslint.config.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --config jest.test.config.js"
  },
  "author": "Malcolm Laing <malcolmclaing@gmail.com> (https://malcolmlaing.com/)",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-knobs": "^4.1.6",
    "@storybook/addon-options": "^4.1.6",
    "@storybook/react": "^4.1.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.3",
    "jest": "23.6.0",
    "jest-dom": "3.0.0",
    "jest-plugin-filename": "0.0.1",
    "jest-runner-eslint": "0.7.1",
    "jest-silent-reporter": "0.1.1",
    "jest-validate": "23.6.0",
    "jest-watch-master": "1.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "1.15.3",
    "prop-types": "15.6.2",
    "raf": "3.4.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-testing-library": "5.4.4",
    "rimraf": "2.6.3",
    "rollup": "1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "webpack": "^4.28.4"
  }
}
